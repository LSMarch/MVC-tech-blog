<div class="row">
    {{#each posts}}
    <div class="card post-card" style="width: 18rem;">
        <div class="card-header post-title">
            {{title}}
        </div>
        <div class="card-body">
            {{!-- <h6 class="card-subtitle mb-2 text-muted">{{topic}}</h6> --}}
            <p class="card-text">{{contents}}</p>
            {{#each  posts.users as |users| }}
            <p class="card-text">{{users.username}}</p>
            {{/each}}
            <a href="posts/{{id}}">View Post</a>
        </div>
    </div>
    {{/each}}
</div>
